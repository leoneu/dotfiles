[user]
	name = Leo Neumeyer
	email = leoneu@apache.org
[core]
	excludesfile = ~/.gitignore_global
	editor = emacs -nw
[alias]
	co = checkout
	st = status
	ci = commit
	ru = remote update
	br = branch
	cam = commit -a -m
	praise = blame
	staged = diff --cached
	unstaged = diff
	both = diff HEAD
	oneline = log --oneline
	amend = commit --amend
	undo = reset --hard HEAD^
	tree = log --graph --decorate --pretty=oneline --abbrev-commit --all
	myhist = !git log --author=\"$(git config user.name)\" --format=%H |xargs git show --name-only --format=-------------%n%Cred%s%Creset%n%Cblue%h%Creset
[push]
	default = tracking
[godiff]
	external = go-diff
[branch]
	autosetupmerge = true
[log]
	decorate = short
[color]
	ui = auto
	interactive = auto
	diff = auto
	branch = auto
	status = auto
[pager]
	status = true
	show-branch = true
[format]
	numbered = auto
[credential]
	helper = store
